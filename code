-- SQLite does NOT support CREATE DATABASE or USE
-- Directly create the table

CREATE TABLE students (
    id INTEGER PRIMARY KEY,
    name TEXT,
    email TEXT,
    age INTEGER
);

INSERT INTO students (id, name, email, age) VALUES
